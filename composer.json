{
  "name": "galatanovidiu/mcp-adapter-implementation-example",
  "description": "MCP Adapter Implementation Example: A WordPress plugin demonstrating MCP Adapter integration and implementation patterns.",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=8.1",
    "automattic/jetpack-autoloader": "^5.0",
    "wordpress/mcp-adapter": "dev-trunk",
    "wordpress/abilities-api": "dev-trunk"
  },
  "autoload": {
    "psr-4": {
      "OvidiuGalatan\\McpAdapterExample\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/WordPress/mcp-adapter"
    },
    {
      "type": "vcs",
      "url": "https://github.com/WordPress/abilities-api"
    }
  ],
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "automattic/jetpack-autoloader": true,
      "composer/installers": true
    },
    "platform": {
      "php": "8.1"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
