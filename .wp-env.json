{
  "$schema": "https://schemas.wp.org/trunk/wp-env.json",
  "core": null,
  "phpVersion": "7.4",
  "mappings": {
    "/wp-content/plugins/mcp-adapter-implementation-example": ".",
    "/wp-content/lib/mcp-adapter": "../../lib/mcp-adapter",
    "/wp-content/lib/abilities-api": "../../lib/abilities-api",
    "/wp-content/debug.log": "../../debug.log"
  },
  "plugins": [
    "./"
  ],
  "themes": [
    "WordPress/twentytwentyfour"
  ],
  "port": 8890,
  "testsPort": 8891,
  "env": {
    "development": {
      "config": {
        "WP_DEBUG": true,
        "WP_DEBUG_LOG": "./wp-content/debug.log",
        "WP_DEBUG_DISPLAY": true,
        "SCRIPT_DEBUG": true,
        "WP_DEVELOPMENT_MODE": "plugin"
      }
    },
    "tests": {
      "config": {
        "WP_DEBUG": true,
        "WP_DEBUG_LOG": "./wp-content/debug.log",
        "WP_DEBUG_DISPLAY": true,
        "SCRIPT_DEBUG": true,
        "FS_METHOD": "direct"
      }
    }
  }
}
